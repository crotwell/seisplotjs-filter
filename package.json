{
  "name": "seisplotjs-filter",
  "version": "1.2.0-alpha.1",
  "description": "Basic timesseries functions for seismic data.",
  "main": "./lib/index.js",
  "dependencies": {
    "js-quantities": "^1.7.2",
    "oregondsp": "^1.1.0",
    "seisplotjs-model": "^1.2.0"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "pretest": "npm run prepare",
    "test": "qunit -c ./lib/index.js -t ./test/index.js",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/index.js --standalone seisplotjs_filter -o seisplotjs_filter_${npm_package_version}_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs-filter.git"
  },
  "keywords": [
    "seismology",
    "miniseed",
    "fft",
    "dsp",
    "filter",
    "seismogram"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs-filter/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs-filter",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "qunit": "^1.0.0"
  }
}
